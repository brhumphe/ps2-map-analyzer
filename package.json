{
  "type": "module",
  "dependencies": {
    "leaflet": "^1.9.4",
    "vue": "^3.5.17",
    "vuetify": "^3.9.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/leaflet": "^1.9.19",
    "@types/node": "^24.0.7",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-vue-devtools": "^7.7.7"
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--experimental-global-webcrypto vite build",
